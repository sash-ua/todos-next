{
  "name": "ToDosNext",
  "version": "0.0.1",
  "homepage": "https://github.com/sash-ua/todos-next",
  "description": "To Do List based on monad-ts library and angust module",
  "scripts": {
    "test:once": "karma start karma.conf.js --single-run",
    "build": "rimraf dist && webpack --config config/webpack.dev.js --bail",
    "build:prod": "rimraf dist && webpack --config config/webpack.prod.js --bail",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build:prod",
    "start": "npm-run-all -p wds copy-dist-files",
    "wds": "webpack-dev-server",
    "pretest": "npm run build",
    "test": "karma start karma.webpack.conf.js",
    "test:ci": "karma start karma.webpack.conf.js",
    "pretest:once": "npm run build",
    "build:watch": "tsc -p src/ -w",
    "build:upgrade": "tsc",
    "serve:upgrade": "http-server",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "copy-dist-files": "node ./copy-dist-files.js",
    "i18n": "ng-xi18n",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "ng": "ng",
    "e2e": "ng e2e"
  },
  "keywords": [
    "monads",
    "typescript",
    "state",
    "monad-ts",
    "functional-js",
    "angular",
    "redux",
    "angust",
    "typescript"
  ],
  "contributors": [
    {
      "name": "Alex Tranchenko",
      "email": "tranchenkoa@gmail.com"
    }
  ],
  "license": "MIT",
